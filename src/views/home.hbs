<h1>Danh sách chấm công</h1>

<!-- Form chấm công -->
<h2>Chấm công</h2>
<form action="/clockIn" method="POST">
  <label for="employeeID">Employee ID:</label>
  <input type="number" id="employeeID" name="employeeID" required>
  <button type="submit">Chấm công</button>
</form>

<!-- Danh sách bản ghi chấm công -->
<h2>Bản ghi chấm công</h2>
<ul>
  {{#each clockInRecords}}
    <li>Nhân viên ID: {{this.EmployeeID}} - Thời gian chấm công: {{this.ClockInTime}}</li>
  {{/each}}
</ul>